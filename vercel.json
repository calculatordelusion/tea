{
  "build": {
    "env": {
      "VITE_DEEPSEEK_V3_API_KEY": "@vite-deepseek-v3-api-key",
      "VITE_DEEPSEEK_R1_API_KEY": "@vite-deepseek-r1-api-key"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "origin-when-cross-origin"
        }
      ]
    }
  ]
}
